define inline_script stimuli
	set description "Executes Python code"
	___run__
	for target_nr, target_canvas in enumerate(target_canvas_list):
		t0 = target_canvas.show()
		if target_nr != len(target_canvas_list) - 1:
			clock.sleep(295)
		else:
			items['last_word'].canvas = target_canvas
	__end__
	___prepare__
	target_canvas_list = []
	for target in var.stimulus.split(' ', 3 ):
		target_canvas = canvas() # add global style arguments here, comma-separated
		target_canvas.rect(color=u'black', fill=False, h=250, penwidth=3, w=250, x=-1280, y=-720)
		target_canvas.rect(color=u'black', fill=False, h=250, penwidth=3, w=250, x=1030, y=-720)
		target_canvas.text('w', color=u'black', center=True, x=-1159, y=-623, font_family='sans', font_size=100)
		target_canvas.text('f', color=u'black', center=True, x=1161, y=-607, font_family='sans', font_size=100)
		target_canvas.text("<span style='color:rgba(0,0,0,.01)'>gb</span>" + target + "<span style='color:rgba(0,0,0,.01)'>gb</span>")
		target_canvas_list.append(target_canvas)
	__end__

