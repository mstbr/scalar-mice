define form_text_display instructions1
	set timeout infinite
	set spacing 10
	set rows "1;4;1"
	set only_render no
	set ok_text OK
	set margins "50;50;50;50"
	set form_title "<span size=24></span>"
	set form_text "Bitte lesen Sie die folgenden Instruktionen aufmerksam durch und versuchen Sie, diese während des Experimentverlaufs so nah wie möglich zu befolgen."
	set description "A simple text display form"
	set cols "1;1;1"
